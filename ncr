#include <bits/stdc++.h>
using namespace std;
#define ll long long
const ll mod = 998244353;
vector<ll> fact(1e5 + 1);
vector<ll> invfact(1e5 + 1);
ll power(ll a, ll n)
{
   if (n == 0)
      return 1;
   ll k = power(a, n / 2);
   if (n % 2 == 0)
      return (k * k) % mod;
   else
      return (((k * k) % mod) * a) % mod;
}

ll ncr(ll n, ll r)
{
   if (r == 0 || r > n)
      return 1;
   ll ans = ((fact[n] * invfact[r]) % mod * invfact[n - r]) % mod;
   return ans;
}

void calc()
{
   fact[0] = 1;
   invfact[0] = 1;
   for (ll i = 1; i <= 1e5; i++)
   {
      fact[i] = (fact[i - 1] * i) % mod;
   }
   invfact[1e5] = power(fact[1e5],mod-2);
   for (int i = 1e5 - 1; i >= 0; i--)
      invfact[i] = (invfact[i + 1] * (i + 1)) % mod;
}

int main()
{
   calc();
   return 0;
}
